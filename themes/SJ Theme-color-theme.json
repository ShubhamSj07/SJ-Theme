    "gitDecoration.deletedResourceForeground": "#f27983b3",
    "gitDecoration.untrackedResourceForeground": "#a6cc70b3",
    "gitDecoration.ignoredResourceForeground": "#525a69",
    "statusBar.foreground": "#fac761"
